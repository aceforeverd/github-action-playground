EXAMPLE_H_	src/example.h	/^#define EXAMPLE_H_$/;"	d
HelloWorld	src/example.py	/^class HelloWorld(object):$/;"	c
PlayGround	src/example.h	/^    explicit PlayGround(int id, std::string name):$/;"	f	struct:example::PlayGround
PlayGround	src/example.h	/^struct PlayGround {$/;"	s	namespace:example
__init__	src/example.py	/^    def __init__(self, id, name):$/;"	m	class:HelloWorld
all	Makefile	/^all:$/;"	t
example	src/example.h	/^namespace example {$/;"	n
example	src/example.java	/^public class example$/;"	c
foo	Makefile	/^foo:$/;"	t
github-action-playground	README.md	/^# github-action-playground$/;"	c
id_	src/example.h	/^    int id_;$/;"	m	struct:example::PlayGround	typeref:typename:int
linters	README.md	/^# linters$/;"	c
main	src/example.cc	/^int main()$/;"	f	typeref:typename:int
main	src/example.java	/^    public static void main(String[] args) {$/;"	m	class:example
name_	src/example.h	/^    std::string name_;$/;"	m	struct:example::PlayGround	typeref:typename:std::string
play	src/example.h	/^    int play() {$/;"	f	struct:example::PlayGround	typeref:typename:int
play	src/example.py	/^    def play(self):$/;"	m	class:HelloWorld
